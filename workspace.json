{
  "version": 1,
  "projects": {
    "brenden-codes": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/brenden-codes",
      "sourceRoot": "apps/brenden-codes",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/brenden-codes",
            "outputPath": "dist/apps/brenden-codes"
          },
          "configurations": {
            "production": {},
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "brenden-codes:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "brenden-codes:build:production",
              "dev": false
            },
            "development": {
              "buildTarget": "brenden-codes:build:development",
              "dev": true
            }
          },
          "defaultConfiguration": "development"
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "brenden-codes:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/brenden-codes/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "brenden-dev": {
      "root": "apps/brenden-dev",
      "sourceRoot": "apps/brenden-dev",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/brenden-dev",
            "outputPath": "dist/apps/brenden-dev"
          },
          "configurations": {
            "development": {},
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "brenden-dev:build",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "brenden-dev:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "brenden-dev:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "brenden-dev:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/brenden-dev/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": []
    },
    "brenden-dev-old": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/brenden-dev-old",
      "sourceRoot": "apps/brenden-dev-old",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/brenden-dev-old",
            "outputPath": "dist/apps/brenden-dev-old"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/brenden-dev-old/environments/environment.ts",
                  "with": "apps/brenden-dev-old/environments/environment.prod.ts"
                }
              ]
            },
            "development": {}
          },
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "brenden-dev-old:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "brenden-dev-old:build:production",
              "dev": false
            },
            "development": {
              "buildTarget": "brenden-dev-old:build:development",
              "dev": true
            }
          },
          "defaultConfiguration": "development"
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "brenden-dev-old:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/brenden-dev-old/**/*.spec.ts",
              "apps/brenden-dev-old/**/*.spec.tsx",
              "apps/brenden-dev-old/**/*.spec.js",
              "apps/brenden-dev-old/**/*.spec.jsx",
              "apps/brenden-dev-old/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/brenden-dev-old/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/brenden-dev-old"]
        }
      },
      "tags": []
    },
    "brenden-fyi": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "application",
      "root": "apps/brenden-fyi",
      "sourceRoot": "apps/brenden-fyi/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/brenden-fyi",
            "index": "apps/brenden-fyi/src/index.html",
            "main": "apps/brenden-fyi/src/main.ts",
            "polyfills": "apps/brenden-fyi/src/polyfills.ts",
            "tsConfig": "apps/brenden-fyi/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/brenden-fyi/src/favicon.ico",
              "apps/brenden-fyi/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/brenden-fyi/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/brenden-fyi/src/environments/environment.ts",
                  "with": "apps/brenden-fyi/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "brenden-fyi:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "brenden-fyi:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "brenden-fyi:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/brenden-fyi/src/**/*.ts",
              "apps/brenden-fyi/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/brenden-fyi"],
          "options": {
            "jestConfig": "apps/brenden-fyi/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "static-render": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "npm run brenden-fyi:postinstall",
              "npx ts-node ./apps/brenden-fyi/setEnv.ts --environment prod",
              "npx nx build brenden-fyi --prod",
              "npx scully --project brenden-fyi"
            ]
          }
        }
      },
      "tags": []
    },
    "image": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/image",
      "sourceRoot": "libs/image/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/image/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/image"],
          "options": {
            "jestConfig": "libs/image/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "storyblok": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/storyblok",
      "sourceRoot": "libs/storyblok/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/storyblok/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/storyblok"],
          "options": {
            "jestConfig": "libs/storyblok/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "utility": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/utility",
      "sourceRoot": "libs/utility/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utility/tsconfig.lib.json",
              "libs/utility/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utility/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utility/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/utility"]
        }
      },
      "tags": []
    }
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json"
}
